{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": ".",
  "framework": null,
  "installCommand": "pip install -r SmartPantryWeb/requirements.txt",
  "builds": [
    {
      "src": "SmartPantryWeb/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "SmartPantryWeb/app.py"
    }
  ]
}
